{
   "name": "Page Highlighter",
   "description": "Highlight all double clicked words",
   "version": "0.1",
   "permissions": [
      "activeTab", "http://*/*", "https://*/*"
   ],
   "content_scripts": [
      {
         "matches": ["https://*/*", "https://*/*"],
         "js": ["myscript.js"],
         "css" : ["mystyles.css"],
         "run_at": "document_end"
      }
   ],
   "browser_action": {
    "default_title": "Highlight Selection"
   },
   "manifest_version": 2
}
